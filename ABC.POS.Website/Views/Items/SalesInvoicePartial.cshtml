    function GetAllMatchedItems(event) {

        var naming = $(".searchnameP").val();
        if (naming == "") {
            $(".item_name").parent().hide();
        }
        else {
            var text = naming;
            var items = $(".item_name");
            //first, hide all:
            items.parent().hide();

            //show only those matching user input:
            items.filter(function () {
                return $(this).text().toLowerCase().indexOf(text) == 0;
            }).parent().show();
        }

    };
