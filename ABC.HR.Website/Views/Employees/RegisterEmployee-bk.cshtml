<script>

    $(document).ready(function () {

        $("#martialstatus").change(function () {
            debugger
            if ($("#martialstatus option:selected")[0].value == "Married") {

                $("#SpouseName").attr("disabled", false);
                $("#NoofChildren").attr("disabled", false);

            } else {
                $("#SpouseName").attr("disabled", true);
                $("#NoofChildren").attr("disabled", true);
            }
        })
    })
    setTimeout(function () {
        $("#msgalert").hide();
    }, 3500);

    $('input.percon').on('change', function () {
        $('input.percon').not(this).prop('checked', false);
    });


    function GetEmployer() {
        $.ajax({
            type: "GET",
            url: "/Administration/AbcDiscounts/GetEmployerForAjax",
            contentType: "application/json;",
            async: false,
            dataType: 'json',
            cache: false,
            success: function (data) {
                console.log(data);
                if (data != "false.") {
                    if (data.federalEmployeeId != null) {
                        $('#FedEmp').val(data.federalEmployeeId);
                        $('#FedEmp').attr("disabled", true);
                    }
                    if (data.employerState != null) {
                        $('#State').val(data.employerState);
                        $('#State').attr("disabled", true);
                    }
                    if (data.employerName != null) {
                        $('#EmpName').val(data.employerName);
                        $('#EmpName').attr("disabled", true);
                    }
                    if (data.payrolAddress != null) {
                        $('#PayRollAddress').val(data.payrolAddress);
                        $('#PayRollAddress').attr("disabled", true);
                    }
                    if (data.city != null) {
                        $('#EmpCity').val(data.city);
                        $('#EmpCity').attr("disabled", true);
                    }
                    if (data.employerZipCode != null) {
                        $('#ZipCode').val(data.employerZipCode);
                        $('#ZipCode').attr("disabled", true);
                    }
                    if (data.employerPhone != null) {
                        $('#EmpPhone').val(data.employerPhone);
                        $('#EmpPhone').attr("disabled", true);
                    }
                    if (data.extention != null) {
                        $('#EmpExt').val(data.extention);
                        $('#EmpExt').attr("disabled", true);
                    }
                    if (data.fax != null) {
                        $('#Fax').val(data.fax);
                        $('#Fax').attr("disabled", true);
                    }
                    if (data.employerEmail != null) {
                        $('#Email').val(data.employerEmail);
                        $('#Email').attr("disabled", true);
                    }
                }
            },
            error: function (error) {
            }
        });
    }


</script>
