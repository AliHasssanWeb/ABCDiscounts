<script>

    $(document).ready(function () {

        $("#martialstatus").change(function () {
            debugger
            if ($("#martialstatus option:selected")[0].value == "Married") {

                $("#SpouseName").attr("disabled", false);
                $("#NoofChildren").attr("disabled", false);

            } else {
                $("#SpouseName").attr("disabled", true);
                $("#NoofChildren").attr("disabled", true);
            }
        })
    })
    setTimeout(function () {
        $("#msgalert").hide();
    }, 3500);

    $('input.percon').on('change', function () {
        $('input.percon').not(this).prop('checked', false);
