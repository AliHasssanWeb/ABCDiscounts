<script>
    function checkYesContractor() {
        $("#YesContractor").prop("checked", true);
        $("#NoContractor").prop("checked", false);
    }
    function checkNoContractor() {
        $("#YesContractor").prop("checked", false);
        $("#NoContractor").prop("checked", true);
    }
</script>
