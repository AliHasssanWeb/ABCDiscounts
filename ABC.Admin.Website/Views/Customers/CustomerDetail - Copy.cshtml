                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="">ZipCode</label>
                            @Html.EditorFor(model => model.Zip, new { htmlAttributes = new { @class = "form-control", placeholder = "ZipCode", id = "zipcode", Readonly = "Readonly" } })
