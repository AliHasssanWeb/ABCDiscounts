                                        <div class="col-md-4">
                                            <label>
                                                Country of Issue
                                            </label>
                                            @Html.EditorFor(model => model.CertificateExemptionInstructions.CountryIssue, new { htmlAttributes = new { @class = "form-control bottomline", placeholder = "Country of Issue", Readonly = "Readonly" } })
                                            @Html.ValidationMessageFor(model => model.CertificateExemptionInstructions.CountryIssue, "", new { @class = "text-danger" })
                                            <hr class="mt-0" />

                                        </div>
                                    </div>
                                    <label>
                                        If no Tax ID Number, enter one of the following:
                                    </label>
                                    <div class="row my-3">
                                        <div class="col-md-4">
                                            <label>
                                                FEIN
                                            </label>
                                            @Html.EditorFor(model => model.CertificateExemptionInstructions.Fein, new { htmlAttributes = new { @class = "form-control bottomline", placeholder = "FEIN", Readonly = "Readonly" } })
                                            @Html.ValidationMessageFor(model => model.CertificateExemptionInstructions.Fein, "", new { @class = "text-danger" })
                                            <hr class="mt-0" />
                                        </div>
                                        <div class="col-md-4">
                                            <label>
                                                Driving License #/State Issued ID #
                                            </label>
                                            @Html.EditorFor(model => model.CertificateExemptionInstructions.Fein, new { htmlAttributes = new { @class = "form-control bottomline", placeholder = "Driving License Number/State Issued ID Number", Readonly = "Readonly" } })
                                            @Html.ValidationMessageFor(model => model.CertificateExemptionInstructions.DrivingLicenseNo, "", new { @class = "text-danger" })
                                            <hr class="mt-0" />
                                        </div>
                                        <div class="col-md-4">
                                            <label>
                                                Country of Issue
                                            </label>
                                            @Html.EditorFor(model => model.CertificateExemptionInstructions.Fein, new { htmlAttributes = new { @class = "form-control bottomline", placeholder = "FEIN Country of Issue", Readonly = "Readonly" } })
                                            @Html.ValidationMessageFor(model => model.CertificateExemptionInstructions.FeinCountry, "", new { @class = "text-danger" })
                                            <hr class="mt-0" />
                                        </div>
                                    </div>
                                    <div class="my-3 row">
                                        <div class="col-md-6">
                                            <label>
                                                Name of seller from whom you are purchasing, leasing, or renting
                                            </label>
                                            @Html.EditorFor(model => model.CertificateExemptionInstructions.SellerName, new { htmlAttributes = new { @class = "form-control bottomline", placeholder = "Name of Seller", Readonly = "Readonly" } })
                                            @Html.ValidationMessageFor(model => model.CertificateExemptionInstructions.SellerName, "", new { @class = "text-danger" })
                                            <hr class="mt-0" />
                                        </div>
                                        <div class="col-md-6">
                                            <label>
                                                Seller Adress
                                            </label>
                                            @Html.EditorFor(model => model.CertificateExemptionInstructions.SellerAdress, new { htmlAttributes = new { @class = "form-control bottomline", placeholder = "Seller Address", Readonly = "Readonly" } })
                                            @Html.ValidationMessageFor(model => model.CertificateExemptionInstructions.SellerAdress, "", new { @class = "text-danger" })
                                            <hr class="mt-0" />
                                        </div>
                                    </div>
                                    <div class="row my-3">
                                        <div class="col-md-4">
                                            <label>
                                                City
                                            </label>
                                            @Html.EditorFor(model => model.CertificateExemptionInstructions.SellerCity, new { htmlAttributes = new { @class = "form-control bottomline", placeholder = "Seller City", Readonly = "Readonly" } })
                                            @Html.ValidationMessageFor(model => model.CertificateExemptionInstructions.SellerCity, "", new { @class = "text-danger" })
                                            <hr class="mt-0" />
                                        </div>
                                        <div class="col-md-4">
                                            <label>
                                                State
                                            </label>
                                            @Html.EditorFor(model => model.CertificateExemptionInstructions.SellerState, new { htmlAttributes = new { @class = "form-control bottomline", placeholder = "Seller State", Readonly = "Readonly" } })
                                            @Html.ValidationMessageFor(model => model.CertificateExemptionInstructions.SellerState, "", new { @class = "text-danger" })
                                            <hr class="mt-0" />
                                        </div>
                                        <div class="col-md-4">
                                            <label>
                                                Zip Code
                                            </label>
                                            @Html.EditorFor(model => model.CertificateExemptionInstructions.SellerZipCode, new { htmlAttributes = new { @class = "form-control bottomline", placeholder = "Seller ZipCode", Readonly = "Readonly" } })
                                            @Html.ValidationMessageFor(model => model.CertificateExemptionInstructions.SellerZipCode, "", new { @class = "text-danger" })
                                            <hr class="mt-0" />
                                        </div>
                                    </div>
                                </div>
                            </li>
                            <li>
                                <div class="row my-3">
                                    <label><b>Type of business. </b>Check the number that describes your business.</label><p></p>
                                    <div class="row">
                                        @foreach (var item in Model.CertificateBusinessTypes.ToList().Where(m => m.CustomerId == Model.Id).ToList())
                                        {
                                            if (item.Type == "Accommodation And Food Services")
                                            {
                                                <div class="col-md-6">
                                                    <input type="checkbox" value="true" checked="checked" disabled="disabled" />
                                                    <label for="AccommodationAndFoodServices">Accommodation and food services</label>
                                                </div>
                                            }
                                            if (item.Type == "Agricultural Forestry Fishing And Hunting")
                                            {
                                                <div class="col-md-6">
                                                    <input type="checkbox" value="true" checked="checked" disabled="disabled" />
                                                    <label for="AgriculturalforestryFishingAndHunting">Agricultural Forestry Fishing And Hunting</label>
                                                </div>
                                            }
                                            if (item.Type == "Construction")
                                            {
                                                <div class="col-md-6">
                                                    <input type="checkbox" value="true" checked="checked" disabled="disabled" />
                                                    <label for="Construction">Construction</label>
                                                </div>
                                            }
                                            if (item.Type == "Finance And Insurance")
                                            {
                                                <div class="col-md-6">
                                                    <input type="checkbox" value="true" checked="checked" disabled="disabled" />
                                                    <label for="FinanceAndInsurance">Finance And Insurance</label>
                                                </div>
                                            }
                                            if (item.Type == "Information Publishing And Communications")
                                            {
                                                <div class="col-md-6">
                                                    <input type="checkbox" value="true" checked="checked" disabled="disabled" />
                                                    <label for="InformationPublishingAndCommunications">Information Publishing And Communications</label>
                                                </div>
                                            }
                                            if (item.Type == "Manufacturing")
                                            {
                                                <div class="col-md-6">
                                                    <input type="checkbox" value="true" checked="checked" disabled="disabled" />
                                                    <label for="Manufacturing">Manufacturing</label>
                                                </div>
                                            }
                                            if (item.Type == "Mining")
                                            {
                                                <div class="col-md-6">
                                                    <input type="checkbox" value="true" checked="checked" disabled="disabled" />
                                                    <label for="Mining">Mining</label>
                                                </div>
                                            }
                                            if (item.Type == "RealEstate")
                                            {
                                                <div class="col-md-6">
                                                    <input type="checkbox" value="true" checked="checked" disabled="disabled" />
                                                    <label for="Manufacturing">RealEstate</label>
                                                </div>
                                            }
                                            if (item.Type == "RealEstate")
                                            {
                                                <div class="col-md-6">
                                                    <input type="checkbox" value="true" checked="checked" disabled="disabled" />
                                                    <label for="Manufacturing">Manufacturing</label>
                                                </div>
                                            }
