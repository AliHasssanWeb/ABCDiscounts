                        </div>
                    </div>

                </div>
                <div class="row">

                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="">Cigratte License No</label>
                            @Html.EditorFor(model => model.CigaretteLicenseNumber, new { htmlAttributes = new { @class = "form-control", placeholder = "Cigratte License No", id = "cigrattelicencenumber", Readonly = "Readonly" } })
